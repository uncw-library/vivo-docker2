:vivodocumentModifier_venue
    a   <java:edu.cornell.mannlib.vitro.webapp.searchindex.documentBuilding.SelectQueryDocumentModifier> ,
        <java:edu.cornell.mannlib.vitro.webapp.searchindex.documentBuilding.DocumentModifier> ;
    rdfs:label "venue" ;
    :hasTargetField "venue_s" ;
    :hasSelectQuery """
        PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>
        PREFIX core: <http://vivoweb.org/ontology/core#>
        SELECT ?venueName
        WHERE {
            ?uri core:hasPublicationVenue ?venue .
            ?venue rdfs:label ?venueName .
        }
        LIMIT 1
        """ .


:vivodocumentModifier_date
    a   <java:edu.cornell.mannlib.vitro.webapp.searchindex.documentBuilding.SelectQueryDocumentModifier> ,
        <java:edu.cornell.mannlib.vitro.webapp.searchindex.documentBuilding.DocumentModifier> ;
    rdfs:label "date" ;
    :hasTargetField "date_dt" ;
    :hasSelectQuery """
        PREFIX xsd:      <http://www.w3.org/2001/XMLSchema#>
        PREFIX core: <http://vivoweb.org/ontology/core#>
        SELECT ?sdate
        WHERE {
            ?uri core:dateTimeValue ?dateTimeValue .
            ?dateTimeValue core:dateTime ?date .
            BIND (CONCAT(STR(xsd:dateTime(?date)), "Z") as ?sdate)
        }
        LIMIT 1
        """ .
